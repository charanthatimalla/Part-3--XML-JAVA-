<!DOCTYPE person [
          <!ELEMENT person (name, age, address)>
                    <!ELEMENT name (#PCDATA)>
                              <!ELEMENT age (#PCDATA)>
                                        <!ELEMENT address (street, city, state, zip)>
                                                  <!ELEMENT street (#PCDATA)>
                                                            <!ELEMENT city (#PCDATA)>
                                                                      <!ELEMENT state (#PCDATA)>
                                                                                <!ELEMENT zip (#PCDATA)>
]>
